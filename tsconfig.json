{
    "compilerOptions": {
        "target": "ES2022",
        "module": "commonjs",
        "lib": [
            "ES2022"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "noUncheckedIndexedAccess": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "types": [
            "node",
            "@playwright/test"
        ],
        "baseUrl": ".",
        "paths": {
            "@fixtures/*": [
                "./fixtures/*"
            ],
            "@pages/*": [
                "./pages/*"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "@config/*": [
                "./config/*"
            ]
        }
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}