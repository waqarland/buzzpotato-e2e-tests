# Test Environment Variables
BASE_URL=https://buzzpotato.online
TEST_USER_EMAIL=testuser@buzzpotato.test
TEST_USER_PASSWORD=SecurePassword123!

# OAuth Testing
# Set to 'mock' to use mocked OAuth flows (no real LinkedIn login needed)
# Set to 'real' for integration testing against actual OAuth providers
OAUTH_MODE=mock
MOCK_OAUTH_ENABLED=true

# LinkedIn OAuth (only needed if OAUTH_MODE=real)
LINKEDIN_CLIENT_ID=your-client-id
LINKEDIN_CLIENT_SECRET=your-client-secret
LINKEDIN_TEST_ACCOUNT_EMAIL=test@example.com
LINKEDIN_TEST_ACCOUNT_PASSWORD=your-test-password

# Supabase (for API-based setup and testing)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
